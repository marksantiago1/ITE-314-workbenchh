SELECT * FROM SERVICE_REQUEST WHERE STATUS = 'OPEN';


SELECT * FROM owner WHERE OWNER_NUM IN ('SM72', 'BL72', 'KE22');


SELECT * FROM marina_slip WHERE RENTAL_FEE > 3000;


SELECT SERVICE_ID, DESCRIPTION, SLIP_ID, (EST_HOURS * 25) AS TOTAL_EST_HR FROM service_request;
